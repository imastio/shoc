{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:11144"
      },
      "Grpc": {
        "Url": "http://*:11146",
        "Protocols": "Http2"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Self": {
    "Application": "shoc-secret",
    "ExternalBaseAddress": "http://localhost:11144/"
  },
  "Discovery": {
    "Type": "static"
  },
  "StaticDiscovery": {
    "HostingType": "localhost",
    "DefaultProtocol": "http",
    "DefaultGrpcProtocol": "http"
  },
  "DataSource": {
    "ConnectionString": "Server=localhost;Port=11001;Uid=shoc;Pwd=shoc;",
    "Database": "shoc"
  },
  "Auth": {
    "Authority": "https://localhost:11109/",
    "Audience": "shoc-secret",
    "AllowInsecure": true,
    "SkipIssuerValidation": false
  },
  "Client": {
    "ClientId": "machine",
    "ClientSecret": "machine",
    "Scope": "svc"
  }
}